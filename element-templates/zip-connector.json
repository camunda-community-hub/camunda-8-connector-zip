{
  "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
  "name": "Zip function",
  "id": "io.camunda.connector.zip.ZipFunction",
  "description": "Different CMIS functions available: Zip,Unzip",
  "documentationRef": "https://docs.camunda.io/docs/components/modeler/web-modeler/connectors/available-connectors/template/",
  "icon": {
    "contents": "data:image/png;base64,\u003d"
  },
  "category": {
    "id": "connectors",
    "name": "Connectors"
  },
  "appliesTo": [
    "bpmn:ServiceTask"
  ],
  "elementType": {
    "value": "bpmn:ServiceTask"
  },
  "groups": [
    {
      "id": "input_file",
      "label": "Input file"
    },
    {
      "id": "parameters",
      "label": "Parameters"
    },
    {
      "id": "destination_storage",
      "label": "Destination Storage"
    },
    {
      "id": "Input",
      "label": "Input"
    },
    {
      "id": "Output",
      "label": "Output"
    }
  ],
  "properties": [
    {
      "binding": {
        "type": "zeebe:taskDefinition:type"
      },
      "type": "Hidden",
      "value": "c-zip-function"
    },
    {
      "binding": {
        "key": "resultVariable",
        "type": "zeebe:taskHeader"
      },
      "type": "Hidden",
      "value": "result"
    },
    {
      "id": "zipFunction",
      "label": "Zip Function",
      "description": "Choose the function to execute",
      "type": "Dropdown",
      "choices": [
        {
          "name": "Zip",
          "value": "zip"
        },
        {
          "name": "Unzip",
          "value": "unzip"
        }
      ],
      "binding": {
        "name": "zipFunction",
        "type": "zeebe:input"
      },
      "constraints": {
        "notEmpty": true
      },
      "group": "Input"
    },
    {
      "condition": {
        "property": "zipFunction",
        "oneOf": [
          "zip"
        ]
      },
      "id": "listSourceFile",
      "label": "List Source files",
      "description": "Variable who contains the list of files to be stored in the Zip file",
      "type": "String",
      "feel": "optional",
      "binding": {
        "name": "listSourceFile",
        "type": "zeebe:input"
      },
      "constraints": {
        "notEmpty": true
      },
      "group": "input_file"
    },
    {
      "condition": {
        "property": "zipFunction",
        "oneOf": [
          "zip"
        ]
      },
      "id": "encoding",
      "label": "Encoding (Chartset",
      "description": "Encoding",
      "value": "UTF-8",
      "type": "String",
      "feel": "optional",
      "binding": {
        "name": "encoding",
        "type": "zeebe:input"
      },
      "group": "parameters"
    },
    {
      "condition": {
        "property": "zipFunction",
        "oneOf": [
          "zip"
        ]
      },
      "id": "stopAtFirstError",
      "label": "Stop at first error",
      "description": "Stop at the first error, else continue to process all other files, and the files in error is not in the ZIP",
      "value": "true",
      "type": "Dropdown",
      "choices": [
        {
          "name": "Yes",
          "value": "true"
        },
        {
          "name": "No",
          "value": "false"
        }
      ],
      "binding": {
        "name": "stopAtFirstError",
        "type": "zeebe:input"
      },
      "group": "parameters"
    },
    {
      "condition": {
        "property": "zipFunction",
        "oneOf": [
          "zip"
        ]
      },
      "id": "zipFileName",
      "label": "Zip File Name",
      "description": "File Name of the Zip file produced",
      "type": "String",
      "feel": "optional",
      "binding": {
        "name": "zipFileName",
        "type": "zeebe:input"
      },
      "constraints": {
        "notEmpty": true
      },
      "group": "parameters"
    },
    {
      "id": "destinationJsonStorageDefinition_optional",
      "label": "Provide JSon Storage Destination?",
      "description": "Storage Definition in Json.",
      "value": "false",
      "binding": {
        "name": "destinationJsonStorageDefinition_optional",
        "type": "zeebe:input"
      },
      "type": "Dropdown",
      "choices": [
        {
          "name": "Yes",
          "value": "true"
        },
        {
          "name": "No",
          "value": "false"
        }
      ],
      "group": "destination_storage"
    },
    {
      "condition": {
        "property": "destinationJsonStorageDefinition_optional",
        "equals": "true"
      },
      "id": "destinationJsonStorageDefinition",
      "label": "JSon Storage Destination",
      "description": "Storage Definition in Json.",
      "type": "String",
      "feel": "optional",
      "binding": {
        "name": "destinationJsonStorageDefinition",
        "type": "zeebe:input"
      },
      "group": "destination_storage"
    },
    {
      "condition": {
        "property": "zipFunction",
        "oneOf": [
          "unzip"
        ]
      },
      "id": "sourceFile",
      "label": "Source file to unzip",
      "description": "Variable referenced the Zip file",
      "type": "String",
      "feel": "optional",
      "binding": {
        "name": "sourceFile",
        "type": "zeebe:input"
      },
      "constraints": {
        "notEmpty": true
      },
      "group": "input_file"
    },
    {
      "condition": {
        "property": "zipFunction",
        "oneOf": [
          "unzip"
        ]
      },
      "id": "sortZipEntries",
      "label": "Sort the list of files in the zip",
      "description": "Sort the list of files in the list.",
      "type": "Dropdown",
      "choices": [
        {
          "name": "Ascii",
          "value": "ASCII"
        },
        {
          "name": "Number",
          "value": "NUMBER"
        },
        {
          "name": "None",
          "value": "NONE"
        }
      ],
      "binding": {
        "name": "sortZipEntries",
        "type": "zeebe:input"
      },
      "constraints": {
        "notEmpty": true
      },
      "group": "parameters"
    },
    {
      "condition": {
        "property": "zipFunction",
        "oneOf": [
          "unzip"
        ]
      },
      "id": "filterFile",
      "label": "Filter file",
      "description": "Filter to unzip only some specific file, like *.jpg to get only image file",
      "type": "String",
      "feel": "optional",
      "binding": {
        "name": "filterFile",
        "type": "zeebe:input"
      },
      "group": "parameters"
    },
    {
      "condition": {
        "property": "zipFunction",
        "oneOf": [
          "unzip"
        ]
      },
      "id": "filterPath",
      "label": "Filter Path",
      "description": "Filter to get only files in a folder, when the zip contains folder",
      "type": "String",
      "feel": "optional",
      "binding": {
        "name": "filterPath",
        "type": "zeebe:input"
      },
      "group": "parameters"
    },
    {
      "condition": {
        "property": "zipFunction",
        "oneOf": [
          "zip"
        ]
      },
      "id": "zipFile",
      "label": "Destination variable name",
      "description": "Process variable where files\u0027s reference is saved",
      "type": "String",
      "feel": "optional",
      "binding": {
        "source": "\u003d result.zipFile",
        "type": "zeebe:output"
      },
      "constraints": {
        "notEmpty": true
      },
      "group": "Output"
    },
    {
      "condition": {
        "property": "zipFunction",
        "oneOf": [
          "unzip"
        ]
      },
      "id": "listDocumentsId",
      "label": "List of documents unzipped",
      "description": "List of file\u0027s reference",
      "type": "String",
      "feel": "optional",
      "binding": {
        "source": "\u003d result.listDocumentsId",
        "type": "zeebe:output"
      },
      "constraints": {
        "notEmpty": true
      },
      "group": "Output"
    },
    {
      "label": "ControllerPage Expression",
      "description": "Expression to define BPMN Errors to throw",
      "type": "Hidden",
      "value": "if is defined(error) then bpmnError(error.code, error.message) else null",
      "binding": {
        "key": "errorExpression",
        "type": "zeebe:taskHeader"
      }
    }
  ]
}